<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EffectiveFromGreaterThanTo</fullName>
    <active>true</active>
    <errorConditionFormula>Effective_Date_From__c  &gt;  Effective_Date_To__c</errorConditionFormula>
    <errorDisplayField>Effective_Date_From__c</errorDisplayField>
    <errorMessage>Effective Date from should be less than Effective Date To</errorMessage>
</ValidationRule>
