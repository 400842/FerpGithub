<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Override_Reason_Other</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Override_Reason_Other__c),
ISPICKVAL(Override_Reason__c , &apos;Other&apos;))</errorConditionFormula>
    <errorDisplayField>Override_Reason_Other__c</errorDisplayField>
    <errorMessage>Please enter an Override Reason (Other)</errorMessage>
</ValidationRule>
